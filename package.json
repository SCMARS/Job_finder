{
  "name": "job-automation-system",
  "version": "1.0.0",
  "description": "Automated job search system with Bundesagentur integration, CAPTCHA solving, and contact enrichment",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install",
    "setup": "./install.sh",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "echo \"No linting configured\" && exit 0"
  },
  "dependencies": {
    "2captcha": "^3.0.5-2",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^128.0.0",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^24.16.2",
    "tesseract.js": "^6.0.1",
    "whois": "^2.13.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "job-automation",
    "bundesagentur",
    "api-integration",
    "google-sheets",
    "apollo",
    "instantly",
    "pipedrive",
    "captcha-solving",
    "web-scraping",
    "contact-enrichment"
  ],
  "author": "Job Automation Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/job-automation-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/job-automation-system/issues"
  },
  "homepage": "https://github.com/your-username/job-automation-system#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
